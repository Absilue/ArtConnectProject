<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.multi.artConnect.reservation.ReservationDAO">
  <insert id="insertReservation" parameterType="reservationVO">
    INSERT INTO `reservation` (`galleryID`, `programID`, `memberID`, `programTitle`,
    `payment`, `reservationTime`, `reservationDay`, `totalPrice`, `adultCount`, `teenagerCount`, `childCount`)
    VALUES (#{galleryID}, #{programID}, #{memberID, jdbcType=NULL}, #{programTitle},
    0, #{reservationTime}, #{reservationDay}, #{totalPrice}, #{adultCount}, #{teenagerCount}, #{childCount})
  </insert>
</mapper>
